{
  "name": "stijl",
  "version": "0.5.0",
  "description": "Chrome extension showing all your code reviews in a single page.",
  "homepage": "https://github.com/google/stijl",
  "bugs": {
    "url": "https://github.com/google/stijl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/stijl"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "browserify src/index.js -o extension/dashboard.js -t [ babelify --plugins [ transform-object-rest-spread ] --presets [ es2015 react ] ]",
    "watch": "watchify src/index.js -v -d -o extension/dashboard.js -t [ babelify --plugins [ transform-object-rest-spread ] --presets [ es2015 react ] ]",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Shuhei Takahashi <takahashi.shuhei@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.1",
    "jquery": "^2.2.4",
    "react": "^15.2.1",
    "react-addons-update": "^15.2.1",
    "react-bootstrap": "^0.30.0-rc.2",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.1",
    "watchify": "^3.7.0"
  }
}
